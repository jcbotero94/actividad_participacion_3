def calcular_media_aritmetica(lista):
    if not lista:
        return None  # Si la lista está vacía, devuelve None

    suma_total = sum(lista)
    cantidad_numeros = len(lista)
    media = suma_total / cantidad_numeros
    return media

# Ejemplo de uso
lista_numeros = [12, 45, 6, 78, 23, 9, 67]
media = calcular_media_aritmetica(lista_numeros)

print("La media aritmética es:", media)